{
  "name": "sura/corner",
  "description": "Generic PHP Exceptions and Errors, made user-friendly.",
  "license": "MIT",
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Sura\\Corner\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sura\\Corner\\Tests\\": "test/"
    }
  },
  "require": {
    "php": "^8.4 || ^8.5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpunit/phpunit": "^12.5",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "*"
  }
}
